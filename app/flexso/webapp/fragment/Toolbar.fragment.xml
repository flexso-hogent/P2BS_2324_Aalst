    xmlns:core="sap.ui.core"
>
    <Toolbar id="_IDGenToolbar1">
        <ToolbarSpacer id="_IDGenToolbarSpacer1" />

        <Button
            id="languageButton"
            icon="sap-icon://globe"
            press="onDropdownPress"
        />
        <Button
            id="_IDGenButton3"
            icon="sap-icon://home"
            press="onBackToHome"
        />
        <Button
            id="_IDGenButton4"
            icon="sap-icon://person-placeholder"
            press="onProfileButtonClick"
        />
        <Button
            id="_IDGenButton8"
            icon="sap-icon://log"
            press="onLogoutPress"
        />
    </Toolbar>
</core:FragmentDefinition>
