<mvc:View
    controllerName="flexso.controller.Editevent"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:tnt="sap.tnt"
    xmlns:core="sap.ui.core"
    displayBlock="true"
>
    <Page
        id="editEventPage"
        title="{i18n>editEventTitle}"
        enableScrolling="true"
    >
        <headerContent>
            <core:Fragment
                fragmentName="flexso.fragment.Toolbar"
                type="XML"
            />
        </headerContent>

        <Title
            id="_IDGenTitle1"
            text="TEST"
        />

        <content />
    </Page>
    <!-- Popover Definition -->
    <core:Fragment
        fragmentName="flexso.fragment.Popover"
        type="XML"
    />
</mvc:View>
