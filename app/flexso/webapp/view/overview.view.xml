            <items>
                <StandardListItem id="_IDGenStandardListItem1" title="{eventModel>eventName}" info="{eventModel>eventDate}" description=" {eventModel>description} "> </StandardListItem>
            </items>
        </List>
    </Page>
</mvc:View>
