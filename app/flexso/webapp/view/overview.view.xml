<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    controllerName="flexso.controller.Overview"
>
    <Page
        id="overviewPage"
        title="{i18n>overview}"
    >
        <headerContent>
            <Toolbar id="_IDGenToolbar1">
                <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                <Button id="_IDGenBackHomeButton" text="{i18n>backToHomeButton}" press="onBackToHome" />
                  <Button
                    id="_IDGenProfileButton"
                    icon="sap-icon://person-placeholder"
                    press="onProfileButtonClick"
                />
            </Toolbar>
        </headerContent>
        <List id="eventList" items="{eventModel>/}">
            <items>
                <StandardListItem id="_IDGenStandardListItem1" title="{eventModel>Name}" info="{eventModel>SDate} until {eventModel>EDate} " description="{eventModel>Description}" />
            </items>
        </List>
    </Page>
</mvc:View>
